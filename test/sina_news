 ["450503", "贵港"], ["450504", "贺州"], ["460100", "海口"], ["460200", "三亚"], ["460300", "西沙群岛"], ["510101", "成都"], ["513321", "眉山"], ["513101", "雅安"], ["510301", "自贡"], ["500100", "重庆"], ["500103", "涪陵"], ["512901", "南充"], ["510501", "泸州"], ["510601", "德阳"], ["510701", "绵阳"], ["510901", "遂宁"], ["511001", "内江"], ["511101", "乐山"], ["512501", "宜宾"], ["510801", "广元"], ["513021", "达州"], ["513401", "资阳"], ["510401", "攀枝花"], ["510402", "阿坝"], ["510403", "甘孜"], ["510405", "广安"], ["510406", "巴中"], ["500239", "黔江"], ["520101", "贵阳"], ["520200", "六盘水"], ["522201", "铜仁"], ["522501", "安顺"], ["522601", "凯里"], ["522701", "都匀"], ["522301", "兴义"], ["522421", "毕节"], ["522101", "遵义"], ["530101", "昆明"], ["530201", "德宏"], ["532301", "楚雄"], ["532401", "玉溪"], ["532501", "红河"], ["532621", "文山"], ["532721", "思茅"], ["532101", "昭通"], ["532821", "西双版纳"], ["532901", "大理"], ["533001", "保山"], ["533121", "怒江"], ["533221", "丽江"], ["533521", "临沧"], ["540101", "拉萨"], ["542121", "昌都"], ["542301", "日喀则"], ["542421", "那曲"], ["542621", "林芝"], ["610101", "西安"], ["610201", "铜川"], ["610301", "宝鸡"], ["610401", "咸阳"], ["612101", "渭南"], ["612301", "汉中"], ["612401", "安康"], ["612601", "延安"], ["612701", "榆林"], ["620101", "兰州"], ["620401", "白银"], ["620301", "金昌"], ["620501", "天水"], ["622201", "张掖"], ["622301", "武威"], ["622421", "定西"], ["622701", "平凉"], ["622901", "临夏"], ["622102", "酒泉"], ["630100", "西宁"], ["632121", "果洛"], ["632321", "格尔木"], ["632521", "海东"], ["632721", "玉树"],  ["640101", "银川"], ["640201", "石嘴山"], ["642101", "吴忠"], ["642221", "固原"], ["650101", "乌鲁木齐"], ["650201", "克拉玛依"], ["652101", "吐鲁番"], ["652201", "哈密"], ["652301", "昌吉"], ["652701", "博乐"], ["652801", "库尔勒"], ["652901", "阿克苏"], ["653101", "喀什"], ["654101", "伊犁"], ["655001", "石河子"], ["655002", "塔城"], ["655003", "阿勒泰"], ["710001", "台北"], ["211001", "辽阳"], ["653201", "和田"], ["820000", "澳门"], ["810000", "香港"]];

              function FillProvinces(a) {
                a.options[0] = new Option("省份", "000000");
                for (i = 0; i < Provinces.length; i++) {
                  a.options[i + 1] = new Option(Provinces[i][1], Provinces[i][0])
                }
                a.options[0].selected = true;
                a.length = i + 1
              }
              function FillCitys(a, b) {
                if (b == "110000" || b == "120000" || b == "310000" || b == "810000" || b == "820000" || b == "500000") {
                  count = 0
                } else {
                  a.options[0] = new Option("城市", b);
                  count = 1
                }
                for (i = 0; i < Citys.length; i++) {
                  if (Citys[i][0].toString().substring(0, 2) == b.substring(0, 2)) {
                    a.options[count] = new Option(Citys[i][1], Citys[i][1]);
                    count = count + 1
                  }
                }
                a.options[0].selected = true;
                a.length = count
              }
              function Province_onchange() {
                FillCitys(g_selCity, g_selProvince.value)
              }
              function InitCitySelect(a, b) {
                g_selProvince = a;
                g_selCity = b;
                a.onchange = function(){Province_onchange()};
                FillProvinces(a);
                Province_onchange()
              }
              function InitCitySelect2(a, b, c) {
                InitCitySelect(a, b);
                for (i = 0; i < a.length; i++) {
                  if (a.options[i].value.substring(0, 2) == c.substring(0, 2)) {
                    a.options[i].selected = true
                  }
                }
                Province_onchange();
                for (i = 0; i < b.length; i++) {
                  if (b.options[i].value == c) {
                    b.options[i].selected = true
                  }
                }
              }
              customCity.requestWea();
              InitCitySelect(document.customize.province,document.customize.city);
              })();
            </script>

<!--XWEATHER_ENDX-->

  		</div>

