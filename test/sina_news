<script>
jsLoader(ARTICLE_JSS.ssologin).jsLoader({url:ARTICLE_JSS.user_panel,charset:'utf-8',callback:function(){

	var userPanel = window.SINA_USER_PANEL;
	var $ = userPanel.STK;

	if(userPanel){
